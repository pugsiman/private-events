<h1>New event</h1>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <%= render 'form' %>
    </div>
  </div>
</div>
<script type="text/javascript">
  $(document).ready(function () {
    $('.datepicker').pickadate({
      format: "Date: dd/mm/yyyy",
      formatSubmit: 'dd/mm/yyyy',
      hiddenName: true
    });
    $('.timepicker').pickatime({
      min: [6,00],
      max: [23,30]
    });
  });
</script>
